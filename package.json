{
  "name": "fastify-extract-definitions",
  "version": "0.0.1",
  "description": "Retrieves TypeScript definitions from Fastify router schema",
  "keywords": [
    "fastify",
    "extract",
    "definitions",
    "ts",
    "typescript",
    "typings",
    "JSON Schema",
    "router",
    "routes"
  ],
  "main": "./build/src",
  "repository": "git@github.com:neruchev/fastify-extract-definitions.git",
  "author": "Kirill Neruchev <kirill@neruchev.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --resolve-plugins-relative-to .",
    "prettier": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "fastify-routes": "^3.0.1",
    "json-schema-to-typescript": "^10.1.4",
