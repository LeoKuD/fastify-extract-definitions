  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fastify": "^3.19.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
